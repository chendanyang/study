import org.lwjgl.opengl.Display;
import org.lwjgl.opengl.DisplayMode;


public class Example {
	float x =400;
	float y =350;
	float rotation =0.0f;
	
	long last_frame;
	int fps;
	long last_fps;
	
	public void start(){
		try{
			LibraryLoader.load();
			Display.setDisplayMode(new DisplayMode(800,600));
			Display.create();
		}catch(Exception e){
			System.out.println("Damn it!");
		}
	}
	
}
